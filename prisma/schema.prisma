  @@index([type, time], map: "type")
  @@index([user, time], map: "user")
  @@index([wid, x, z, time], map: "wid")
}

model cp_material_map {
  rowid    Int     @id @default(autoincrement())
  id       Int?
  material String? @db.VarChar(255)

  @@index([id], map: "id")
}

model cp_session {
  rowid  Int  @id @default(autoincrement())
  time   Int?
  user   Int?
  wid    Int?
  x      Int?
  y      Int?
  z      Int?
